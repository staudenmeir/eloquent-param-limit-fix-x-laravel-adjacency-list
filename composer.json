{
    "name": "staudenmeir/eloquent-param-limit-fix-x-laravel-adjacency-list",
    "description": "Merge of staudenmeir/eloquent-param-limit-fix and staudenmeir/laravel-adjacency-list",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "staudenmeir/eloquent-param-limit-fix": "^v1.1.1",
        "staudenmeir/laravel-adjacency-list": "^1.21"
    },
    "require-dev": {
        "laravel/framework": "^11.0|^12.0",
        "orchestra/testbench-core": "^9.5",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\EloquentParamLimitFixXLaravelAdjacencyList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staudenmeir\\EloquentParamLimitFix\\Tests\\": "tests/EloquentParamLimitFix",
            "Staudenmeir\\LaravelAdjacencyList\\Tests\\": "tests/LaravelAdjacencyList"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
